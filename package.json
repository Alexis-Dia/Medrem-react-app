{
  "name": "medrem-react-app",
  "version": "0.0.1",
  "description": "medrem react app",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "scripts": {
    "compile": "node build/scripts/compile",
    "build": "set NODE_OPTIONS=--max_old_space_size=2048 && cross-env NODE_ENV=production webpack --config webpack.prod.js --progress",
    "build:test": "set NODE_OPTIONS=--max_old_space_size=2048 && npm run test && cross-env NODE_ENV=production webpack --config webpack.prod.js --progress",
    "build-linux": "export NODE_OPTIONS=--max_old_space_size=2048 && cross-env NODE_ENV=production webpack --config webpack.prod.js --progress",
    "start": "cross-env NODE_ENV=development webpack serve --config webpack.dev.js --progress",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "check-types": "tsc",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
